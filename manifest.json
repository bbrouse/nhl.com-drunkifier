{
  "manifest_version": 2,

  "name": "Nhl.com Drunkifier",
  "description": "This extension drunkifies the team icons on NHL.com",
  "version": "1.0",
  
  "content_scripts": [
    {
      "matches": ["http://www.nhl.com/*"],
      "css": [],
      "js": ["drunkify.js"]
    }
  ],
  "web_accessible_resources": [
       "team_logo_nav_drunkified.png"
  ]
}